.content {
  display: flex;
  flex-direction: column;
  width: 100%;
}

.content-user {
  display: flex;
  flex-direction: row;
  width: 100%;
  background: white;
  align-items: center;
  height: 100%;
  background-color: #3caa35; 
}

.content-user label {
  text-align: center;
  margin-top: 20px;
  font-weight: bold;
  font-size: 15px;
  color: white;
}

.content-user text {
  text-align: center;
  font-weight: bolder;
  font-family: Arial, Helvetica, sans-serif;
  font-size: 50rpx;
  color:#dfc158;
}
.user-info{
  padding:0 30rpx;
}
.money{
  margin-top: 6px;
}
.divide {
  height: 16px;
  background: #eaeaea;
}

.item-btn {
  display: flex;
  align-items: center;
  width: 100%;
  height: 50px;
}

.item-btn navigator {
  display: flex;
  align-items: center;
  padding-left: 16px;
  padding-right: 16px;
  font-size: 16px;
  width: 100%;
  height: 50px;
  position: relative;
}
.arrowright{
  width:14px;
  height:20px;
  position:absolute;
  top:16px;
  right:20px;
}
.nav-icon{
  width: 22px;
  height:35px;
  margin-right: 10px;
}
.navigator-hover{
  background-color: #f1f1f1;
}